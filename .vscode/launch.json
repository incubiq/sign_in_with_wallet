    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "SAMPLE APP",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/backend_sample/server.js",
            "args": ["app_secret=someotherkey_1234567890"]            
        },
 
        {
            "type": "node",
            "request": "launch",
            "name": "SIWW oAuth",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/backend_siww/server.js",
            "args": ["cache_secret=mysecretforcache", "jwt_secret=mysecretforjwt"]            
        }
    ]
}